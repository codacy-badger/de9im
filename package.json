    "test:lint": "eslint ./test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dpmcmlxxvi/de9im.git"
  },
  "keywords": [
    "DE-9IM",
    "spatial",
    "predicate",
    "relations"
