  "author": "Daniel Pulido <dpmcmlxxvi@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dpmcmlxxvi/de9im/issues"
  },
  "homepage": "https://github.com/dpmcmlxxvi/de9im",
  "dependencies": {
    "@turf/turf": "5.1.6",
    "earcut": "2.1.5",
    "geojson-rbush": "2.1.0"
  },
  "devDependencies": {
    "coveralls": "3.0.3",
    "eslint": "5.16.0",
    "eslint-config-google": "0.12.0",
    "esm": "3.2.22",
    "glob": "7.1.3",
    "load-json-file": "6.0.0",
    "np": "5.0.1",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.0",
    "path": "0.12.7",
    "rollup": "1.11.2",
    "rollup-plugin-commonjs": "9.3.4",
    "rollup-plugin-node-resolve": "4.2.3",
